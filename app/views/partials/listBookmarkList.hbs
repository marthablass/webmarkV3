
{{#each bookmarks}}
  <section class="ui segment">
    <h2 class="ui header">
      {{title}}
    </h2>
    <a href="/bookmarkList/{{id}}" class="ui icon button">
      <i class="icon folder open"></i>
    </a>
    <a href="/bookmarkDB/deleteBookmarkList/{{id}}" class="ui icon button">
      <i class="icon trash"></i>
    </a>
  </section>
{{/each}}
  
 