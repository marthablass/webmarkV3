<table class="ui table">
  <thead>
    <tr>
      <th>Bookmark</th>
      <th>Link</th>
    </tr>
  </thead>
  <tbody>
    {{#each bookmarkList.bookmarks}}
      <tr>
        <td>
          {{title}}
        </td>
        <td>
          <a href= "{{link}}" target="_blank">{{link}}</a>
        </td>
      </tr>
    <td>
      <a href="/bookmarkList/{{../bookmarkList.id}}/deletebookmark/{{id}}" class="ui tiny red button">Delete Bookmark</a>
    </td>
      {{/each}}
  </tbody>
</table>